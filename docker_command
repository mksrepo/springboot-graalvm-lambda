docker build -t springboot-graalvm-lambda:latest .

docker create --name temp springboot-graalvm-lambda:latest

docker cp temp:/var/task/bootstrap ./bootstrap

file springboot-graalvm-lambda

chmod +x bootstrap

zip lambda.zip bootstrap

chmod +x deploy-lambda-from-ecr.sh
./deploy-lambda-from-ecr.sh

aws iam attach-role-policy \
  --role-name lambda-basic-execution \
  --policy-arn arn:aws:iam::aws:policy/service-role/AWSLambdaBasicExecutionRole



